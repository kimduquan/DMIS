<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="executors" type="System.Configuration.NameValueFileSectionHandler"/>
  </configSections>
  <appSettings>
    <!-- Repository url. Default: "http://localhost" -->
    <add key="RepositoryUrl" value="http://localhost"/>
    <!-- Username (in the form of domain\username) for accessing the portal -->
    <add key="Username" value="BuiltIn\admin"/>
    <!-- Password for the user above -->
    <add key="Password" value="admin"/>
    <!-- Interval for refreshing lock of the executing task. Default: "15" -->
    <add key="UpdateLockPeriodInSeconds" value="100"/>
    <!-- Maximum executor inactivity. Default: "30" -->
    <add key="ExecutorTimeoutInSeconds" value="600"/>
    <!-- Directory of the task executor exe-s. Relative to the agent exe. Default: "./TaskExecutors" -->
    <add key="TaskExecutorDirectory" value="./TaskExecutors"/>
    <add key="HeartbeatPeriodInSeconds" value="100" />
  </appSettings>
  <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Owin" publicKeyToken="31bf3856ad364e35" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-2.1.0.0" newVersion="2.1.0.0"/>
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Owin.Security" publicKeyToken="31bf3856ad364e35" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-2.0.2.0" newVersion="2.0.2.0"/>
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30AD4FE6B2A6AEED" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0"/>
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
